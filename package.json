{
  "name": "htpx-cli",
  "version": "0.1.3",
  "description": "Terminal HTTP interception toolkit",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "htpx": "dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --dir src",
    "test:int": "vitest run --dir tests/integration",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "http",
    "https",
    "proxy",
    "intercept",
    "mitm",
    "debugging",
    "cli",
    "tui",
    "terminal"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtford90/htpx.git"
  },
  "bugs": {
    "url": "https://github.com/mtford90/htpx/issues"
  },
  "homepage": "https://github.com/mtford90/htpx#readme",
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@ink-tools/ink-mouse": "^2.1.0",
    "better-sqlite3": "^12.6.2",
    "cli-highlight": "^2.1.11",
    "commander": "^14.0.3",
    "ink": "^6.6.0",
    "ink-use-stdout-dimensions": "^1.0.5",
    "mockttp": "^4.2.1",
    "react": "^19.2.4",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "@types/uuid": "^11.0.0",
    "cli-testing-library": "^3.0.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "ink-testing-library": "^4.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  }
}
